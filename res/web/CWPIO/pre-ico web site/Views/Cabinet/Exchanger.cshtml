<div class="row">
<div class="col-md-7">
<div class="m-portlet">
<div class="m-portlet__head">
	<div class="m-portlet__head-caption">
		<div class="m-portlet__head-title">
			<h3 class="m-portlet__head-text">
				Choose currency to purchase CWT-P
			</h3>
		</div>
	</div>
	<div class="m-portlet__head-tools">
		<div class="input-group m-input-group m-input-group--nonborder">
			<div class="m-input-icon m-input-icon--left">
				<input data-bind="textInput: obs.searchInput" placeholder="Search coin..." class="form-control m-input"
				       type="text"/>
				<span class="m-input-icon__icon m-input-icon__icon--left"><span><i class="la la-search"></i></span></span>
			</div>
		</div>
	</div>
</div>
<div class="m-portlet__body">

<div data-bind="ifnot:flags.currenciesReady" class="s-ta_c s-mb_20">
	<div class="m-spinner m-spinner--brand m-spinner--lg"></div>
	<div class="m-spinner m-spinner--primary m-spinner--lg"></div>
	<div class="m-spinner m-spinner--success m-spinner--lg"></div>
	<div class="m-spinner m-spinner--info m-spinner--lg"></div>
	<div class="m-spinner m-spinner--warning m-spinner--lg"></div>
	<div class="m-spinner m-spinner--danger m-spinner--lg"></div>
</div>

<div data-bind="css:{'s-display_bi': flags.currenciesReady}" class="s-display_n">
	
	<div class="row s-overflow_h">
		<div class="col-md-12 col-sm-12">

			<div class="s-mb_40i row">
				<div data-bind="css:{'col-md-5': flags.gateOperating, 'col-md-12': (flags.gateOperating() === false)}" class="b-currencies-wrapper">
					
					<div data-bind="css:{'s-display_bi': flags.emptySearchRequest}" class="s-display_n b-empty-search--coins">
						There is no currencies you are search 
					</div>
					
					<table class="table table-hover table-sm t-coins">
						<tbody>
						<!-- ko foreach: currencies -->
						<tr>
							<td>
								<div>
									<img class="s-icon s-mr_5" data-bind="css:{'grayscale': ($data.status === 'unavailable')},attr:{'src': $data.imageSmall}" src=""/>
									<span class="s-icon s-fw_600" data-bind="text: $data.symbol"></span>
								</div>
							</td>
							<td data-bind="css:{'active': ($root.obs.currentCoin.symbol() === $data.symbol) }, attr:{'colspan': ($data.status === 'unavailable' && 2 || 0)}">
								<!--ko text: $data.name--><!--/ko-->

								<!-- ko if: ($data.status === 'unavailable')-->
								<i>(unavailable)</i>
								<!--/ko-->
							</td>
							<!-- ko ifnot: ($data.status === 'unavailable')-->
							<td class="s-ta_r">
								<button data-bind="disable:$data.status === 'unavailable', click:$root.actions.initGate"
								        class="btn btn-sm btn-outline-focus m-btn--square btn-outline-brand m-btn--outline-2x m-btn m-loader--primary m-loader--right" type="button">
									purchase
								</button>
							</td>
							<!--/ko-->
						</tr>
						<!-- /ko -->

						</tbody>
					</table>
				</div>

				<div data-bind="css:{'col-md-7': (flags.gateOperating() === true), 's-display_n': (flags.gateOperating() === false)}">

					<div data-bind="css:{'slide-up': flags.slideUpLimits, 'slide-down': flags.slideDownLimits}" class="currency-current-wrapper clearfix">
						<div class="currency-current">
							<div class="s-mb_10">
								<img width="100" data-bind="attr:{'src':obs.currentCoin.image}"/>
							</div>
						</div>
						
						
						<div class="currency-current-right">
							<h5 class="m--font-primary">
								<span class="s-icon s-mr_10" data-bind="text: (obs.currentCoin.name() + ' ' + obs.currentCoin.symbol())"></span>
								<a data-bind="click:actions.offGate"
								   href="javascript: void(0)" 
								   data-toggle="tooltip" 
								   title="cancel purchase" 
								   class="s-icon">
									<i class="la la-power-off"></i>
								</a>
							</h5>
							<div style="margin-bottom: 5px">
								<strong data-bind="text: obs.market.limit"></strong> limit
							</div>
							<div style="margin-bottom: 5px">
								<strong data-bind="text: obs.market.maxLimit"></strong> maxLimit
							</div>
							<div style="margin-bottom: 5px">
								<strong data-bind="text: obs.market.minimum"></strong> minimum
							</div>
							<div>
								<strong data-bind="text: obs.market.rate"></strong> rate
							</div>
						</div>
					</div>

					
					<!-- ko if: flags.whiteListLess -->

					<!-- ko ifnot:flags.whiteListAskFormReady -->
					<div class="s-ta_c">
						<div class="m-spinner m-spinner--brand m-spinner--lg"></div>
						<div class="m-spinner m-spinner--primary m-spinner--lg"></div>
						<div class="m-spinner m-spinner--success m-spinner--lg"></div>
						<div class="m-spinner m-spinner--info m-spinner--lg"></div>
						<div class="m-spinner m-spinner--warning m-spinner--lg"></div>
						<div class="m-spinner m-spinner--danger m-spinner--lg"></div>
					</div>
					<!-- /ko -->

					<!-- ko if:flags.whiteListAskFormReady -->
					<div class="b-ask-text">
						<!-- ko text:obs.askFormText --><!-- /ko -->
					</div>
					
					<!-- ko if: flags.whiteListInputReady -->
					<div data-bind="css:{'show-slow': flags.whiteListInputReady}" class="b-ask-wl--input">
						<div class="form-group m-form__group">
							<div class="input-group">
								<input placeholder="Enter your ETH wallet address"
								       maxlength="42" data-bind="textInput:obs.whiteListAddressField" autofocus type="text" class="form-control"/>
					
								<!-- ko if:flags.hasMetamask -->
								<div class="input-group-append">
									<button data-bind="disable:(flags.lockFill() || flags.whiteListAddressProcessing()), click:actions.getndFillAddressFromMetamask" 
									        class="btn  btn-primary" type="button">Fill from MetaMask 
										<img height="16" style="margin-left: 5px;vertical-align: top;" src="~/images/metamask.png"/></button>
								</div>
								<!-- /ko -->
							</div>
						</div>
						
						<!-- ko if:flags.whiteListAddressProcessing -->
						<div class="s-ta_c">
							<div class="m-spinner m-spinner--brand m-spinner--lg"></div>
							<div class="m-spinner m-spinner--primary m-spinner--lg"></div>
							<div class="m-spinner m-spinner--success m-spinner--lg"></div>
							<div class="m-spinner m-spinner--info m-spinner--lg"></div>
							<div class="m-spinner m-spinner--warning m-spinner--lg"></div>
							<div class="m-spinner m-spinner--danger m-spinner--lg"></div>
						</div>
						<!-- /ko -->
					
					</div>
					<!-- /ko -->
					
					<!-- /ko -->

					<!--/ko-->
					
					<!-- ko ifnot: flags.whiteListLess -->
					<!-- ko if:flags.isFixedAmmountMode -->
					
				<div data-bind="css:{'s-display_bi': (flags.expiredOrder() && !flags.userDidTransaction())}" 
				     class="s-display_n m-alert m-alert m-alert--square alert alert-danger alert-dismissible fade show" role="alert">
					<p>Ths Order is expired, do not deposit any currency to address below!</p>
					<a data-bind="click:actions.continueExpiredOrder" class="btn btn-info btn-sm m-btn--square" href="javascript: void(0)">Create new order</a>
				</div>
				
				<div data-bind="css:{'s-display_bi': (flags.expiredOrder() && flags.userDidTransaction())}" 
				     class="s-display_n m-alert m-alert m-alert--square alert alert-info alert-dismissible fade show" role="alert">
					<p>Your transaction in progress, current order is expired, create new order for more CWT-P and do not deposit any currency to address below</p>
					<a data-bind="click:actions.continueExpiredOrder" class="btn btn-info btn-sm m-btn--square" href="javascript: void(0)">Create new order</a>
				</div>
					
				@*					<h6 class="m--font-primary">*@
@*						Optionally you can specify return address(<strong><!-- ko text: obs.currentCoin.symbol --><!--/ko--></strong>) *@
@*					</h6>*@
@*					<div data-bind="css:{'m-loader m-loader--primary m-loader--right': flags.returnAddressValidating}" class="s-mb_20">*@
@*						<input data-bind="textInput: obs.returnAddress, disable:flags.returnAddressValidating" type="text" class="form-control ammount-control"/>*@
@*					</div>*@
					
				<div data-bind="css:{'s-display_bi': (obs.currentCoin.symbol() !== 'ETH')}" class="s-display_n s-mb_20">
					<div class="input-group">
						<div class="input-group-prepend b-receive-before s-c_777">
							Return address (<!-- ko text: obs.currentCoin.symbol --><!--/ko-->)
							
							<div data-bind="css:{'m-loader m-loader--primary': flags.returnAddressValidating}" 
							       style="width: 30px; display: inline-block;"></div>
						</div>
						
							<input placeholder="This field is optional" data-bind="textInput: obs.returnAddress, disable:flags.returnAddressValidating" type="text" class="form-control ammount-control"/>
						
					</div>
				</div>
					
					<div data-bind="css:{'s-display_bi': (!flags.isValidReturnAddress())}" 
					     class="s-display_n m-alert alert alert-danger alert-dismissible fade show" role="alert">
						This is not a valid <strong><!-- ko text: obs.currentCoin.symbol --><!--/ko--></strong> address  	
					</div>
					
@*					<h6 class="m--font-primary">*@
@*						CWT-P Token amount you want to receive*@
@*					</h6>*@
@*					<div class="s-mb_20">*@
@*						<input autofocus data-bind="textInput: obs.cwtCount, disable:flags.depositAddrGetting" type="number" class="form-control ammount-control"/>*@
@*					</div>*@
					
					<div class="s-mb_20">
						<div class="input-group">
							<div class="input-group-prepend  b-receive-before" style="">
								I want to receive
							</div>
							<input autofocus="" data-bind="textInput: obs.cwtCount, disable:flags.depositAddrGetting" type="number" class="form-control ammount-control">
							<div class="input-group-append b-receive-after">CWT-P</div>
						</div>
					</div>
					
					
					<div data-bind="css:{'s-display_bi': flags.minCont}" class="s-display_n m-alert m-alert--square alert alert-info alert-dismissible fade show" role="alert">
						Minimum contribution is 500 CWT-P  	
					</div>
					<!--/ko-->
					<!--/ko-->
					
					<!-- ko if:flags.depositAddrGetting -->
					<div class="s-ta_c">
						<div class="m-spinner m-spinner--brand m-spinner--lg"></div>
						<div class="m-spinner m-spinner--primary m-spinner--lg"></div>
						<div class="m-spinner m-spinner--success m-spinner--lg"></div>
						<div class="m-spinner m-spinner--info m-spinner--lg"></div>
						<div class="m-spinner m-spinner--warning m-spinner--lg"></div>
						<div class="m-spinner m-spinner--danger m-spinner--lg"></div>
					</div>
					<!-- /ko -->
					
					<!-- ko ifnot: flags.whiteListLess -->
					<!-- ko ifnot:flags.depositAddrGetting -->
					<div class="s-mb_20" data-bind="if: flags.depositAddrGot">
						<div class="s-mb_10">
							<!-- ko if: flags.isFixedAmmountMode -->
							<p>
								Please send <!-- ko text: obs.fixedAmmount.depositAmount --><!--/ko--> 
								<strong><!-- ko text: obs.currentCoin.symbol --><!--/ko--></strong>
								(includs fee: <!-- ko ifnot: obs.currentCoin.symbol() === 'ETH' -->0.0022<!--/ko-->
								<!-- ko if: obs.currentCoin.symbol() === 'ETH' -->0<!--/ko--> ETH) to the address below:
							</p>
							<!-- ko if: (obs.currentCoin.symbol() !== 'ETH') -->
							<p>
								This order will expair on 
								<strong>
									<!-- ko text: (new Date(obs.fixedAmmount.expiration()).toLocaleString()) --><!--/ko-->
								</strong>
							</p>
							<!-- /ko -->
							<!-- /ko -->
							
							<!-- ko ifnot: flags.isFixedAmmountMode -->
							Please send your <strong><!-- ko text: obs.currentCoin.symbol --><!--/ko--></strong> to the address below:
							<!-- /ko -->
						</div>
						<div class="s-mb_10">
							<strong data-bind="text:obs.depositAddress" class="s-icon purch-addr" id="purchaseAddr"></strong>
							<!-- ko ifnot: obs.depositAddress -->
								<strong>deposit address error</strong>
							<!--/ko-->
							<!-- ko if: obs.depositAddress -->
							<img src="~/images/copy-ico.svg" class="btn-copy" data-bind="click:actions.notify.bind(null, 'copied!')" 
							     id="copy2" data-clipboard-target="#purchaseAddr"/>
							<!--/ko-->	
						</div>
						<!-- ko if: obs.depositAddress -->
						<div class="pull-left">
							<img width="100" data-bind="attr:{'src':('https://api.qrserver.com/v1/create-qr-code/?size=150x150&amp;data=' + obs.depositAddress())}"
							     style="border: 1px solid #ddd;"/>
						</div>
						<!--/ko-->
						<button data-bind="click:actions.confirmDidTransaction" style="margin-top: 35px;" 
						   class="pull-right btn btn-info btn-sm m-btn--square">
							I did send
						</button>
					</div>
					<!-- /ko -->
					<!-- /ko -->
					

				</div>
			</div>


		</div>
	</div>
</div>

